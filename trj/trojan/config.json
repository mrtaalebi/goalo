{
    "run_type": "server",
    "local_addr": "0.0.0.0",
    "local_port": 443,
    "remote_addr": "fakehttp",
    "remote_port": 80,
    "password": [
    ],
    "log_level": 3,
    "ssl": {
        "cert": "/trojan/fullchain.pem",
        "key": "/trojan/privkey.pem",
        "sni": "${DOMAIN}",
        "session_ticket": true,
        "reuse_session": true,
        "fallback_addr": "fakehttp",
        "fallback_port": 80,
        "fingerprint": "chrome"
    },
    "tcp": {
        "no_delay": true,
        "keep_alive": true
    },
    "mux": {
        "enabled": false,
        "concurrency": 8,
        "idle_timeout": 60
    },
    "router": {
        "enabled": true,
        "proxy": [
            "geosite:geolocation-!ir"
        ],
        "bypass": [
            "geoip:ir",
            "geoip:private",
            "geosite:ir",
            "geosite:geolocation-ir"
        ],
        "block": [
            "geosite:category-ads"
        ]
    },
    "mysql": {
        "enabled": true,
        "server_addr": "mysql",
        "server_port": 3306,
        "database": "trojan",
        "username": "trojan",
        "password": "trojan",
        "check_rate": 60
    },
    "api": {
        "enabled": false,
        "api_addr": "0.0.0.0",
        "api_port": 8080
    }
}
